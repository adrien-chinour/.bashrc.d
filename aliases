# Listing
alias ll="exa -l"
alias la="exa -la"
alias l="ls"

# Divers
alias reset="source ~/.bashrc"

# Docker Compose
alias dcd="docker-compose down"
alias dcu="docker-compose up -d"
alias dc="docker-compose"

# Docker
alias dockerc="docker container"
alias dockeri="docker image"

# Git
alias gs="git status"
alias gl="git log --graph --decorate --oneline --all"
